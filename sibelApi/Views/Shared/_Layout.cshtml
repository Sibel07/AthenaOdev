<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="~/Content/datatable/jquery-1.9.1.js"></script>
    <script src="~/Content/datatable/jquery-ui.js"></script>
    <script src="~/Content/datatable/jquery.dataTables.min.js"></script>
    <script src="~/Content/datatable/dataTables.responsive.js"></script>

    <link href="~/Content/datatable/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/datatable/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/datatable/dataTables.responsive.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
     
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @RenderSection("scripts", required: false)

</body>
</html>
